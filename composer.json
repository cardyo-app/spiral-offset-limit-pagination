{
  "name": "cardyo/spiral-offset-pagination",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "description": "Minimal offset/limit pagination for Spiral DataGrid",
  "keywords": [
    "spiral",
    "pagination",
    "offset",
    "limit",
    "datagrid"
  ],
  "authors": [
    {
      "name": "Leonid Meleshin (leon0399)",
      "email": "hello@leon0399.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "spiral/data-grid": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "spiral/data-grid-bridge": "^3.0",
    "spiral/cycle-bridge": "^2.12",
    "cycle/orm": "^2.11",
    "cycle/database": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "Cardyo\\SpiralOffsetPagination\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cardyo\\Tests\\SpiralOffsetPagination\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
